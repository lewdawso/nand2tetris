/**
 * 4-way 16-bit multiplexor:
 * out = a if sel == 00
 *       b if sel == 01
 *       c if sel == 10
 *       d if sel == 11
 */

CHIP Mux4Way16 {
    IN a[16], b[16], c[16], d[16], sel[2];
    OUT out[16];

    PARTS:
    //select between a and b
    Mux16(a=a[0], b=b[0], sel=sel[0], out=muxab0);
    //select between c and d
    Mux16(a=c[0], b=d[0], sel=sel[0], out=muxcd0);
    //select between a/b and c/d
    Mux16(a=muxab0, b=muxcd0, sel=sel[1], out=out[0]);

    Mux16(a=a[1], b=b[1], sel=sel[0], out=muxab1);
    Mux16(a=c[1], b=d[1], sel=sel[0], out=muxcd1);
    Mux16(a=muxab1, b=muxcd1, sel=sel[1], out=out[1]);

    Mux16(a=a[2], b=b[2], sel=sel[0], out=muxab2);
    Mux16(a=c[2], b=d[2], sel=sel[0], out=muxcd2);
    Mux16(a=muxab2, b=muxcd2, sel=sel[1], out=out[2]);

    Mux16(a=a[3], b=b[3], sel=sel[0], out=muxab3);
    Mux16(a=c[3], b=d[3], sel=sel[0], out=muxcd3);
    Mux16(a=muxab3, b=muxcd3, sel=sel[1], out=out[3]);

    Mux16(a=a[4], b=b[4], sel=sel[0], out=muxab4);
    Mux16(a=c[4], b=d[4], sel=sel[0], out=muxcd4);
    Mux16(a=muxab4, b=muxcd4, sel=sel[1], out=out[4]);

    Mux16(a=a[5], b=b[5], sel=sel[0], out=muxab5);
    Mux16(a=c[5], b=d[5], sel=sel[0], out=muxcd5);
    Mux16(a=muxab5, b=muxcd5, sel=sel[1], out=out[5]);

    Mux16(a=a[6], b=b[6], sel=sel[0], out=muxab6);
    Mux16(a=c[6], b=d[6], sel=sel[0], out=muxcd6);
    Mux16(a=muxab6, b=muxcd6, sel=sel[1], out=out[6]);

    Mux16(a=a[7], b=b[7], sel=sel[0], out=muxab7);
    Mux16(a=c[7], b=d[7], sel=sel[0], out=muxcd7);
    Mux16(a=muxab7, b=muxcd7, sel=sel[1], out=out[7]);

    Mux16(a=a[8], b=b[8], sel=sel[0], out=muxab8);
    Mux16(a=c[8], b=d[8], sel=sel[0], out=muxcd8);
    Mux16(a=muxab8, b=muxcd8, sel=sel[1], out=out[8]);

    Mux16(a=a[9], b=b[9], sel=sel[0], out=muxab9);
    Mux16(a=c[9], b=d[9], sel=sel[0], out=muxcd9);
    Mux16(a=muxab9, b=muxcd9, sel=sel[1], out=out[9]);

    Mux16(a=a[10], b=b[10], sel=sel[0], out=muxab10);
    Mux16(a=c[10], b=d[10], sel=sel[0], out=muxcd10);
    Mux16(a=muxab1, b=muxcd10, sel=sel[1], out=out[10]);

    Mux16(a=a[11], b=b[11], sel=sel[0], out=muxab11);
    Mux16(a=c[11], b=d[11], sel=sel[0], out=muxcd11);
    Mux16(a=muxab11, b=muxcd11, sel=sel[1], out=out[11]);

    Mux16(a=a[12], b=b[12], sel=sel[0], out=muxab12);
    Mux16(a=c[12], b=d[12], sel=sel[0], out=muxcd12);
    Mux16(a=muxab12, b=muxcd12, sel=sel[1], out=out[12]);

    Mux16(a=a[13], b=b[13], sel=sel[0], out=muxab13);
    Mux16(a=c[13], b=d[13], sel=sel[0], out=muxcd13);
    Mux16(a=muxab13, b=muxcd13, sel=sel[1], out=out[13]);

    Mux16(a=a[14], b=b[14], sel=sel[0], out=muxab14);
    Mux16(a=c[14], b=d[14], sel=sel[0], out=muxcd14);
    Mux16(a=muxab14, b=muxcd14, sel=sel[1], out=out[14]);

    Mux16(a=a[15], b=b[15], sel=sel[0], out=muxab15);
    Mux16(a=c[15], b=d[15], sel=sel[0], out=muxcd15);
    Mux16(a=muxab15, b=muxcd15, sel=sel[1], out=out[15]);
}
